  <div class="wrapper">
    <div class="agreement">
      <h2>个人借款协议</h2>
      <!--
      <div class="butt"><c:if test="${borrow.status>=21}"><a href="javascript:agreementExport()">下载</a></c:if></div> -->
      <form id="frm" method="post" action="/xjx/credit-loan/agreement_export" onsubmit="return navTabSearch(this);">
